,"190"],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/components/Footer/Footer.tsx",[],["191","192"],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/Login/Login.tsx",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/Login/translation.ts",["193"],"import Login from \"./Login\";\n\nconst translation: TranslationObject = {\n  \"En-GB\": {\n    LogInPageTitle: \"Login\",\n    LogIn: \"Log in\",\n    email_address: \"Email adress\",\n    enter_email: \"Enter your email\",\n    password: \"Password\",\n    click_to_sign_up: \"Click here to sign up\",\n  },\n  \"Fr-FR\": {\n    LogInPageTitle: \"Connectez-vous\",\n    LogIn: \"Se connecter\",\n    email_address: \"Adresse E-mail\",\n    enter_email: \"Votre adresse email\",\n    password: \"Mot de passe\",\n    click_to_sign_up: \"Cliquez ici pour créer un compte\",\n  },\n};\n\nexport default translation;\n",["194","195"],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/SignUp/SignUp.tsx",["196","197"],"import React, { useState, useEffect } from \"react\";\nimport Form from \"react-bootstrap/Form\";\nimport Container from \"react-bootstrap/Container\";\nimport Button from \"react-bootstrap/Button\";\nimport { login, signUp } from \"../../store/user/actions\";\nimport { selectToken, selectUserLanguage } from \"../../store/user/selectors\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory, Link } from \"react-router-dom\";\nimport { Col } from \"react-bootstrap\";\nimport translation from \"./translation\";\n\nexport default function Login() {\n  const userLanguage: Language = useSelector(selectUserLanguage);\n  const [newUser, setNewUser] = useState<User>({\n    firstName: \"\",\n    lastName: \"\",\n    email: \"\",\n    password: \"\",\n    language: userLanguage,\n    gender: \"other\",\n  });\n  const dispatch = useDispatch();\n  const token = useSelector(selectToken);\n  const history = useHistory();\n\n  const {\n    sign_up_page_title,\n    first_name,\n    last_name,\n    email_address,\n    password_label,\n    language_label,\n    gender_label,\n    sign_up,\n    t_male,\n    t_female,\n    t_other,\n  } = translation[userLanguage];\n\n  useEffect(() => {\n    if (token !== null) {\n      history.push(\"/\");\n    }\n  }, [token, history]);\n\n  function submitForm(event: any) {\n    event.preventDefault();\n\n    dispatch(signUp(newUser));\n  }\n\n  return (\n    <Container>\n      <Form as={Col} md={{ span: 6, offset: 3 }} className=\"mt-5\">\n        <h1 className=\"mt-5 mb-5\">{sign_up_page_title}</h1>\n        <Form.Group controlId=\"formBasicFirstName\">\n          <Form.Label>{first_name}</Form.Label>\n          <Form.Control\n            value={newUser.firstName}\n            onChange={(event) =>\n              setNewUser({ ...newUser, firstName: event.target.value })\n            }\n            placeholder={first_name}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicFirstName\">\n          <Form.Label>{last_name}</Form.Label>\n          <Form.Control\n            value={newUser.lastName}\n            onChange={(event) =>\n              setNewUser({ ...newUser, lastName: event.target.value })\n            }\n            placeholder={last_name}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicEmail\">\n          <Form.Label>{email_address}</Form.Label>\n          <Form.Control\n            value={newUser.email}\n            onChange={(event) =>\n              setNewUser({ ...newUser, email: event.target.value })\n            }\n            type=\"email\"\n            placeholder={email_address}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicLanguage\">\n          <Form.Label>{language_label}</Form.Label>\n          <Form.Control\n            as=\"select\"\n            value={newUser.language}\n            onChange={(event) =>\n              setNewUser({\n                ...newUser,\n                language: event.target.value as Language,\n              })\n            }\n            placeholder={language_label}\n            required\n          >\n            <option value=\"En-GB\">English</option>\n            <option value=\"Fr-FR\">Français</option>\n          </Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId=\"formBasicGender\">\n          <Form.Label>{gender_label}</Form.Label>\n          <Form.Control\n            as=\"select\"\n            value={newUser.gender}\n            onChange={(event) =>\n              setNewUser({ ...newUser, gender: event.target.value as Gender })\n            }\n            required\n          >\n            <option value=\"male\">{t_male}</option>\n            <option value=\"female\">{t_female}</option>\n            <option value=\"other\"> {t_other} </option>\n          </Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId=\"formBasicPassword\">\n          <Form.Label>{password_label}</Form.Label>\n          <Form.Control\n            value={newUser.password}\n            onChange={(event) =>\n              setNewUser({ ...newUser, password: event.target.value })\n            }\n            type=\"password\"\n            placeholder={password_label}\n            required\n          />\n        </Form.Group>\n        <Form.Group className=\"mt-5\">\n          <Button variant=\"primary\" type=\"submit\" onClick={submitForm}>\n            {sign_up}\n          </Button>\n        </Form.Group>\n      </Form>\n    </Container>\n  );\n}\n","/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/SignUp/translation.ts",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/HomePage/HomePage.tsx",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/MyRecipes/translation.ts",[],"/home/thibaudszy/Doc