     history.push(\"/\");\n    }\n  }, [token, history]);\n\n  function submitForm(event: any) {\n    event.preventDefault();\n\n    dispatch(signUp(newUser));\n  }\n\n  return (\n    <Container>\n      <Form as={Col} md={{ span: 6, offset: 3 }} className=\"mt-5\">\n        <h1 className=\"mt-5 mb-5\">{sign_up_page_title}</h1>\n        <Form.Group controlId=\"formBasicFirstName\">\n          <Form.Label>{first_name}</Form.Label>\n          <Form.Control\n            value={newUser.firstName}\n            onChange={(event) =>\n              setNewUser({ ...newUser, firstName: event.target.value })\n            }\n            placeholder={first_name}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicFirstName\">\n          <Form.Label>{last_name}</Form.Label>\n          <Form.Control\n            value={newUser.lastName}\n            onChange={(event) =>\n              setNewUser({ ...newUser, lastName: event.target.value })\n            }\n            placeholder={last_name}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicEmail\">\n          <Form.Label>{email_address}</Form.Label>\n          <Form.Control\n            value={newUser.email}\n            onChange={(event) =>\n              setNewUser({ ...newUser, email: event.target.value })\n            }\n            type=\"email\"\n            placeholder={email_address}\n            required\n          />\n        </Form.Group>\n        <Form.Group controlId=\"formBasicLanguage\">\n          <Form.Label>{language_label}</Form.Label>\n          <Form.Control\n            as=\"select\"\n            value={newUser.language}\n            onChange={(event) =>\n              setNewUser({\n                ...newUser,\n                language: event.target.value as Language,\n              })\n            }\n            placeholder={language_label}\n            required\n          >\n            <option value=\"En-GB\">English</option>\n            <option value=\"Fr-FR\">Français</option>\n          </Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId=\"formBasicGender\">\n          <Form.Label>{gender_label}</Form.Label>\n          <Form.Control\n            as=\"select\"\n            value={newUser.gender}\n            onChange={(event) =>\n              setNewUser({ ...newUser, gender: event.target.value as Gender })\n            }\n            required\n          >\n            <option value=\"male\">{t_male}</option>\n            <option value=\"female\">{t_female}</option>\n            <option value=\"other\"> {t_other} </option>\n          </Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId=\"formBasicPassword\">\n          <Form.Label>{password_label}</Form.Label>\n          <Form.Control\n            value={newUser.password}\n            onChange={(event) =>\n              setNewUser({ ...newUser, password: event.target.value })\n            }\n            type=\"password\"\n            placeholder={password_label}\n            required\n          />\n        </Form.Group>\n        <Form.Group className=\"mt-5\">\n          <Button variant=\"primary\" type=\"submit\" onClick={submitForm}>\n            {sign_up}\n          </Button>\n        </Form.Group>\n      </Form>\n    </Container>\n  );\n}\n","/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/SignUp/translation.ts",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/HomePage/HomePage.tsx",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/MyRecipes/translation.ts",[],"/home/thibaudszy/Documents/Codaisseur/Portfolio_project/frontend/src/pages/MyRecipes/MyRecipes.tsx",["172","173"],"import React, { useEffect } from \"react\";\nimport {\n  Button,\n  Card,\n  Jumbotron,\n  ListGroup,\n  ListGroupItem,\n} from \"react-bootstrap\";\nimport translation from \"./translation\";\nimport { useHistory, Link